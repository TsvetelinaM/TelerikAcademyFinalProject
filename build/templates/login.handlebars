<div class="container-fluid tm-main-header">
<p class="tm-main-header-content">Log In</p>
</div>
<div class="container-fluid tm-main">
    <div class="tm-main-path-name"><span class="tm-grey">Home <span class="tm-strange-symbol"></span></span> LogIn</div>
    <div class="tm-login-control">
        <form class="form-horizontal tm-form" method="post">

            <!-- Text input-->
            <div class="form-group">
                <label class="col-xs-6 col-md-4 control-label" for="tm-textinput-email-log">Email:</label>  
                <div class="col-xs-12 col-md-8">
                    <input id="tm-textinput-email-log" name="tm-textinput-email" type="text" placeholder="Please enter your email" class="tm-textinput-email-log form-control input-md" required="">
                </div>
            </div>

            <!-- Password input-->
            <div class="form-group">
            <label class="col-xs-6 col-md-4 control-label" for="tm-passwordinput-password-log">Password:</label>
            <div class="col-xs-12 col-md-8">
                <input id="tm-passwordinput-password-log" name="tm-passwordinput-password" type="password" placeholder="Please enter your password" class="tm-passwordinput-password-log form-control input-md" required="">
                
            </div>
            </div>

            <!-- Button -->
            <div class="form-group">
            <label class="col-xs-12 col-md-4 control-label" for="tm-singlebutton-login-log"></label>
            <div class="col-xs-6 col-md-4">
                <button id="tm-singlebutton-login-log" name="tm-singlebutton-login" class="btn btn-primary tm-singlebutton-login-log">Login</button>
            </div>
            <div class="col-xs-6 col-md-4 tm-redirect-to">
                or 
                <a href="#/register">Register</a>
            </div>
            </div>

            </form>

    </div>

</div>