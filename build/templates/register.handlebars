<div class="container-fluid tm-main-header">
<p class="tm-main-header-content">Register</p>
</div>
<div class="container-fluid tm-main">
    <div class="tm-main-path-name"><span class="tm-grey">Home <span class="tm-strange-symbol"></span></span> Register</div>
    <div class="tm-login-control">
        <form class="form-horizontal tm-form" method="post">

            <!-- Text input-->
            <div class="form-group">
                <label class="col-xs-6 col-md-4 control-label" for="tm-textinput-username">Username:</label>  
                <div class="col-xs-12 col-md-8">
                    <input id="tm-textinput-username" name="tm-textinput-username" type="text" placeholder="Please enter your username" class="form-control input-md tm-textinput-username" required="">
                </div>
            </div>

            <!-- Text input-->
            <div class="form-group">
                <label class="col-xs-6 col-md-4 control-label" for="tm-textinput-email">Email:</label>  
                <div class="col-xs-12 col-md-8">
                    <input id="tm-textinput-email" name="tm-textinput-email" type="text" placeholder="Please enter your email" class="form-control input-md tm-textinput-email" required="">
                </div>
            </div>

            <!-- Password input-->
            <div class="form-group">
            <label class="col-xs-6 col-md-4 control-label" for="tm-passwordinput-password">Password:</label>
            <div class="col-xs-12 col-md-8">
                <input id="tm-passwordinput-password" name="tm-passwordinput-password" type="password" placeholder="Please enter your password" class="form-control input-md tm-passwordinput-password" required="">
                
            </div>
            </div>

            <!-- Button -->
            <div class="form-group">
            <label class="col-xs-6 col-md-4 control-label" for="tm-singlebutton-register"></label>
            <div class="col-xs-6 col-md-4">
                <button id="tm-singlebutton-register" name="tm-singlebutton-register" class="btn btn-primary tm-singlebutton-register">Register</button>
            </div>
            </div>

        </form>

    </div>
    

</div>